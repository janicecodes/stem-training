#counting lettersin strings
str= "Hello guys welcome back to my class"
for x in str:
    if x !='l':
        print(x)
#counting lettersin strings
str= "Hello guys welcome back to my class"
outstr=""
for x in str:
    if x !='l':
      outstr ==x
print(outstr)
str= "Hello guys welcome back to my class"
outstr=""
for x in str:
    if x !='l' and x !='e' and x !=z'u':
      outstr ==x
print(x)
